    "eslint-plugin-svelte": "2.39.0",
    "postcss": "8.4.38",
    "postcss-import": "16.1.0",
    "prettier": "3.2.5",
    "prettier-plugin-svelte": "3.2.3",
    "prettier-plugin-tailwindcss": "0.5.14",
    "process": "0.11.10",
    "svelte": "4.2.17",
    "svelte-check": "3.7.1",
    "tailwindcss": "3.4.3",
    "tslib": "2.6.2",
    "typescript": "5.4.5",
