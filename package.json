    "@vercel/analytics": "^1.3.1",
    "@web3auth/base": "8.5.0",
    "@web3auth/modal": "8.5.0",
    "@web3auth/openlogin-adapter": "8.5.0",
    "@web3auth/phantom-adapter": "8.5.0",
    "@web3auth/solana-provider": "8.5.0",
    "bits-ui": "^0.21.10",
    "clsx": "2.1.1",
    "drizzle-orm": "0.30.10",
    "drizzle-valibot": "0.2.0",
    "lru-cache": "10.2.2",
    "lucide-svelte": "^0.378.0",
    "postgres": "3.4.4",
    "satori": "^0.10.13",
    "tailwind-merge": "2.3.0",
    "tailwind-variants": "0.2.1",
    "three": "^0.164.1",
    "valibot": "0.30.0"
  },
  "devDependencies": {
    "@playwright/test": "1.44.0",
    "@sveltejs/adapter-auto": "3.2.0",
    "@sveltejs/adapter-vercel": "^5.3.1",
    "@sveltejs/kit": "^2.5.10",
    "@sveltejs/vite-plugin-svelte": "3.1.0",
    "@tailwindcss/forms": "0.5.7",
    "@types/eslint": "8.56.10",
