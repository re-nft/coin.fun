{
  "name": "coin-fun",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "codegen": "playwright codegen",
    "component": "npx shadcn-svelte@latest",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "dev": "concurrently npm:dev:*",
    "dev:app": "vite dev",
    "dev:db": "node --env-file=.env scripts/db",
    "dev:lint": "npm run check -- --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint . && npm run check",
    "postbuild": "[[ -n \"$VERCEL_ENV\" ]] && npm run db:migrate",
    "preview": "vite preview",
    "test": "concurrently -k npm:test:*",
    "test:e2e": "playwright test",
    "test:unit": "vitest"
  },
  "dependencies": {
    "@resvg/resvg-js": "2.6.2",
    "@sentry/sveltekit": "8.9.2",
    "@solana/web3.js": "1.93.0",
    "@supabase/ssr": "0.3.0",
    "@supabase/supabase-js": "2.43.5",
    "@vercel/analytics": "1.3.1",
    "@web3auth/base": "8.6.2",
    "@web3auth/modal": "8.6.2",
    "@web3auth/openlogin-adapter": "8.6.2",
    "@web3auth/phantom-adapter": "8.6.2",
    "@web3auth/solana-provider": "8.6.2",
    "bits-ui": "0.21.10",
    "clsx": "2.1.1",
    "drizzle-orm": "0.31.2",
    "drizzle-valibot": "0.2.0",
    "lru-cache": "10.2.2",
    "lucide-svelte": "0.395.0",
    "postgres": "3.4.4",
    "satori": "0.10.13",
    "tailwind-merge": "2.3.0",
    "tailwind-variants": "0.2.1",
    "three": "0.165.0",
    "valibot": "0.32.0"
  },
  "devDependencies": {
    "@playwright/test": "1.44.1",
    "@sveltejs/adapter-auto": "3.2.2",
    "@sveltejs/adapter-vercel": "5.3.2",
    "@sveltejs/kit": "2.5.16",
    "@sveltejs/vite-plugin-svelte": "3.1.1",
    "@tailwindcss/forms": "0.5.7",
    "@types/eslint": "8.56.10",
    "@types/node": "20.14.2",
    "@types/three": "0.165.0",
    "@typescript-eslint/eslint-plugin": "7.13.0",
    "@typescript-eslint/parser": "7.13.0",
    "autoprefixer": "10.4.19",
    "buffer": "6.0.3",
    "concurrently": "8.2.2",
    "drizzle-kit": "0.22.7",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-simple-import-sort": "12.1.0",
    "eslint-plugin-svelte": "2.40.0",
    "postcss": "8.4.38",
    "postcss-import": "16.1.0",
    "prettier": "3.3.2",
    "prettier-plugin-svelte": "3.2.4",
    "prettier-plugin-tailwindcss": "0.6.5",
    "process": "0.11.10",
    "svelte": "4.2.18",
    "svelte-check": "3.8.0",
    "tailwindcss": "3.4.4",
    "tslib": "2.6.3",
    "typescript": "5.4.5",
    "typescript-svelte-plugin": "0.3.38",
    "vite": "5.3.1",
    "vite-plugin-node-polyfills": "0.22.0",
    "vitest": "1.6.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.18.0"
  },
  "overrides": {
    "@web3auth/ui": {
      "react": "18.2.0",
      "react-dom": "18.2.0"
    }
  },
  "type": "module"
}
