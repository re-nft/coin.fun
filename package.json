{
  "name": "coin-fun",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "component": "npx shadcn-svelte@latest",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "dev": "concurrently npm:dev:*",
    "dev:app": "vite dev",
    "dev:db": "node --env-file=.env scripts/db",
    "dev:lint": "npm run check -- --watch",
    "build": "vite build",
    "postbuild": "[[ -n \"$VERCEL_ENV\" ]] || npm run db:migrate",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "lint": "prettier --check . && eslint . && npm run check",
    "format": "prettier --write .",
    "test": "concurrently -k npm:test:*",
    "test:integration": "playwright test",
    "codegen": "playwright codegen",
    "test:unit": "vitest"
  },
  "dependencies": {
    "@resvg/resvg-js": "2.6.2",
